{
  "name": "lochmueller/error-log",
  "type": "typo3-cms-extension",
  "description": "Log 404 errors for BE users to easily create redirects",
  "require": {
    "php": "^8.1",
	  "typo3/cms-core": "^12.4",
	  "typo3/cms-redirects": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "HDNET\\ErrorLog\\": "Classes/"
    }
  },
  "homepage": "https://www.hdnet.de",
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "preferred-install": {
      "*": "dist"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "error_log",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
